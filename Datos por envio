 <script type="text/javascript">
  $(function () {
   $('.DTP').datepicker({ dateFormat: "dd/mm/yy" });
   ValidNumber('Qty');
   ValidNumber('Valor');
   ValidNumber('Peso');
   ValidNumber('Alto');
   ValidNumber('Ancho');
   ValidNumber('Largo');
  });
  function Cancel(ContainerID, ControlID, Value) {
   $('#' + ContainerID).css('display', 'none');
   $('#' + ControlID).val(Value);
  }
  function Accept(ContainerID) {
   $('#' + ContainerID).css('display', 'none');
   document.getElementById('Frm01').submit();
  }
  function DateCancel(ContainerID, ControlID, Value) {
   $('#' + ContainerID).css('display', 'none');
   if (Value == '01-Jan-01 12:00:00 AM') {
    $('#' + ControlID).val('')
   } else {
    $('#' + ControlID).val(Value);
   }
  }
  function DateAccept(ContainerID) {
   $('#' + ContainerID).css('display', 'none');
   document.getElementById('Frm01').submit();
  }
  function ValidNumber(ID) {
   document.getElementById(ID).onkeydown = function (e) {
    var key = e.keyCode ? e.keyCode : e.which;
    var stat = false;
    if (isNaN(String.fromCharCode(key))) {
     if (key != 8 && key != 190) stat = true;
    }
    if (stat == true) return false;
   }
  }
  function SendEMail(Evento) {
   $('#SendMail').val(Evento);
   document.getElementById('Frm01').submit();
  }
  function Envio_Delete() {
   $('#Action').val('Env_Delete');
   document.getElementById('Frm01').submit();
  }
 </script>

